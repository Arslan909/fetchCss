{
    "manifest_version": 3,
    "name": "CSS Inspector",
    "version": "1.0.1",
    "description": "Inspect CSS properties of elements",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["index.js"]
      }
    ]
  }
  